generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Plants {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  plantName String
  wateringFreq Int
  lastWatered DateTime
  daysSinceWatered Int
  wateredToday Boolean


  @@map("plant-pal")
}
